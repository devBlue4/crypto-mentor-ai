        "@typescript-eslint/parser": "^8.19.1",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "express": "^4.21.2",
        "globals": "^15.14.0",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "lint-staged": "^15.3.0",
        "nodemon": "^3.1.9",
