{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/",
        "dist/",
        "coverage/",
        "LICENSE",
        "package.json",
        "yarn.lock",
        "README.md"
    ],
    "watch": ["./"],
    "execMap": {
        "ts": "node -r ts-node/register"
    },
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
}
